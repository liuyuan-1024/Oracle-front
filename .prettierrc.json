{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "objectProperties": "preserve",
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": true,
  "vueIndentStyle": true,
  "cssIndent": 2,
  "templateCurlySpacing": "always",
  "proseWrap": "preserve",
  "requirePragma": true,
  "insertPragma": true
}